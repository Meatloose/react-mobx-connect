{
  "name": "react-mobx-connect",
  "version": "0.0.1",
  "description": "React connector to MobX",
  "main": "lib/index.js",
  "jsnext:main": "lib/index.es.js",
  "module": "lib/index.es.js",
  "browser": "lib/index.cjs.js",
  "repository": "https://github.com/Meatloose/react-mobx-connect",
  "author": "Meatloose <poiuy_1100@163.com>",
  "license": "MIT",
  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "build:src": "rollup -c rollup.config.prod.js",
    "build:test": "rollup -c rollup.config.test.js",
    "test": "mocha --require jsdom-global/register __test__",
    "watch": "npm-run-all --parallel watch:*",
    "watch:src": "rollup --watch -c rollup.config.dev.js",
    "watch:mocha": " mocha --watch --require jsdom-global/register __test__",
    "watch:test": "rollup --watch -c rollup.config.test.js",
    "pub": "npm run build && npm publish",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "lint:fix": "npm run lint -- --fix"
  },
  "files": [
    "lib",
    "__test__"
  ],
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/mocha": "^5.2.0",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "babel-core": "^6.26.3",
    "chai": "^4.1.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jsdom": "16.5.0",
    "jsdom-global": "3.0.2",
    "mobx": "^4.3.0",
    "mobx-react": "^5.1.2",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.3",
    "postcss-cssnext": "^3.1.0",
    "postcss-nested": "^3.0.0",
    "postcss-simple-vars": "^4.1.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "rollup": "^0.59.3",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-image": "^1.0.2",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-livereload": "^0.6.0",
    "rollup-plugin-local-resolve": "^1.0.7",
    "rollup-plugin-multi-entry": "^2.0.2",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.2.1",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-postcss": "^1.6.1",
    "rollup-plugin-serve": "^0.4.2",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-watch": "^4.3.1",
    "sinon": "^5.0.10",
    "source-map-support": "^0.5.6",
    "tslint": "^5.10.0",
    "tslint-react": "^3.6.0",
    "typescript": "^2.8.3"
  },
  "peerDependencies": {
    "mobx": ">=2",
    "mobx-react": "^3.5.x"
  }
}
